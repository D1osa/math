    "import math\n",
    "\n",
    "def vector_len(*args):\n",
    "    sum = 0\n",
    "    for n in args:\n",
    "        sum += n**2\n",
    "    return math.sqrt(sum)\n",
    "vector_len(1, 2, 4)"
